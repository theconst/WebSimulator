            <transferFunction id="controlSystem">
                <negativeFeedback>
                    <pid id="pid" initial="1.0" p="3.0" i="24.0" />
                    <sequentialConnection>
                        <lag id="seq-lag" initial="0.0" gain="0.7" timeConstant="24" />
                        <delay id="delay" initial="0.0" time="8.0" />
                    </sequentialConnection>
                </negativeFeedback>
            </transferFunction>
            
            <transferFunction id="parallel">
                <parallelConnection>
                    <lag id="parallelLag" initial="5.0" gain="5.0" timeConstant="100.0" />
                    <lag id="parrallelLag2" initial="1.0" gain="2.0" timeConstant="1000.0" />
                    <lag id="parrallelLag3" initial="3.0" gain="5.0" timeConstant="3000.0" />
                </parallelConnection>
            </transferFunction>
            
            <transferFunction id="sequential">
                <sequentialConnection>
                    <lag id="seq-lag" initial="0.0" gain="0.9" timeConstant="100" />
                    <delay id="delay" initial="0.0" time="8.0" />
                </sequentialConnection>
            </transferFunction>
            
            <transferFunction id="noise">
                <!-- do not specify initial for noise -->
                <parallelConnection>
                    <noise id="noiseNoise" initial="0.0" variance="1.0" />
                    <noise id="anotherNoise" initial="0.0" variance="2.0" />
                </parallelConnection>
            </transferFunction>
            
            <transferFunction id="typical1">
                <folpdn id="typicalfolpdn" initial="0.0" gain="1.511" timeConstant="142" delay="24" variance="0.05" />
